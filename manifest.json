{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "0.0.4",
  "author": "leopold.jacquot@gmail.com",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "__MSG_appName__",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}