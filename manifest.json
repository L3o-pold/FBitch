{
  "manifest_version": 2,
  "name": "Facebook ads blocker",
  "version": "0.0.2",
  "author": "leopold.jacquot@gmail.com",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Facebook ads blocker"
  },
  "permissions": ["activeTab"]
}